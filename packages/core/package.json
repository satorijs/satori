{
  "name": "@satorijs/core",
  "description": "Core components of Satorijs",
  "version": "5.0.0-alpha.0",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satorijs/satori.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/satorijs/satori/issues"
  },
  "homepage": "https://satori.chat/",
  "keywords": [
    "satori",
    "element",
    "message",
    "utilities",
    "cordis"
  ],
  "cordis": {
    "ecosystem": {
      "name": "satori",
      "pattern": [
        "@satorijs/adapter-*",
        "@satorijs/plugin-*",
        "satori-adapter-*",
        "satori-plugin-*"
      ]
    },
    "service": {
      "implements": [
        "satori"
      ]
    }
  },
  "peerDependencies": {
    "cordis": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@cordisjs/plugin-http": "^1.2.0"
  },
  "dependencies": {
    "@cordisjs/element": "^0.3.0",
    "@satorijs/protocol": "^2.0.0-alpha.0",
    "cordis": "^4.0.0-beta.3",
    "cosmokit": "^1.8.0",
    "path-to-regexp": "^8.2.0",
    "path-to-regexp-typed": "^8.0.0"
  }
}
